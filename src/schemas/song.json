{
  "Main": {
    "song_title": {
      "type": "Text",
      "config": {
        "label": "Song title",
        "placeholder": "Song or track title"
      }
    },
    "artist": {
      "type": "Text",
      "config": {
        "label": "artist",
        "placeholder": "Artist's name"
      }
    },
    "uid": {
      "type": "UID",
      "config": {
        "label": "slug",
        "placeholder": "URL for song post"
      }
    },
    "timestamp": {
      "type": "Timestamp",
      "config": {
        "label": "timestamp",
        "placeholder": "Time to post the song"
      }
    },
    "author": {
      "type": "Link",
      "config": {
        "select": "document",
        "customtypes": ["author"],
        "label": "author",
        "placeholder": "Who wrote the blurb"
      }
    },
    "album_art": {
      "type": "Image",
      "config": {
        "constraint": {},
        "thumbnails": [],
        "label": "Album Art"
      }
    },
    "excerpt": {
      "type": "Text",
      "config": {
        "label": "excerpt",
        "placeholder": "Short blurb to display at top of the article"
      }
    },
    "content": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph, preformatted, heading1, heading2, heading3, heading4, heading5, heading6, strong, em, hyperlink, image, embed, list-item, o-list-item, o-list-item",
        "label": "Content",
        "placeholder": "The review blurb for that song that will actually be read by whatever audience"
      }
    },
    "spotify_id": {
      "type": "Text",
      "config": {
        "label": "Spotify song ID",
        "placeholder": "Track id on Spotify"
      }
    },
    "soundcloud_url": {
      "type": "Text",
      "config": {
        "label": "Soundcloud url",
        "placeholder": "Track url on Soundcloud"
      }
    },
    "tag_list": {
      "type": "Group",
      "config": {
        "fields": {
          "all_tags": {
            "type": "Link",
            "config": {
              "select": "document",
              "customtypes": ["tag"],
              "label": "tags"
            }
          }
        },
        "label": "tag_list"
      }
    },
    "body": {
      "type": "Slices",
      "fieldset": "Slice zone",
      "config": {
        "choices": {}
      }
    }
  }
}
